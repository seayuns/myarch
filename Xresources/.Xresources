!!URL操作插件1 
!操作： Alt + u  当前URL   
!k/j 上/下一条url  
!o 打开url 
URxvt.perl-ext: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/google-chrome-stable
URxvt.url-select.underline: true
!使用matcher通过键盘打开和列出最近(目前限制为10个)的url
!URxvt.keysym.C-Delete: perl:matcher:last
!URxvt.keysym.M-Delete: perl:matcher:list
!URl 定义插件2
!URxvt.perl-ext-common: default,matcher
!URxvt.url-launcher: /usr/bin/google-chrome-stable
!URxvt.matcher.button: 1
!URL配色
!URxvt.matcher.rend.0: Uline Bold fg5


!插件：Simple tabs选项卡
!URxvt.perl-ext-common: tabbed
!URxvt.tabbed.tabbar-fg: 2
!URxvt.tabbed.tabbar-bg: 0
!URxvt.tabbed.tab-fg: 3
!URxvt.tabbed.tab-bg: 0

!!滚动条设置
URxvt.scrollBar:False
URxvt.scrollBar_floating:False
URxvt.scrollstyle:plain
!!滚屏设置
URxvt.mouseWheelScrollPage:True
URxvt.scrollTtyOutput:False
URxvt.scrollWithBuffer:True
URxvt.scrollTtyKeypress:True
!！启用第二屏幕
!URxvt.secondaryScreen: 1
!URxvt.secondaryScroll: 0

!!光标闪烁
URxvt.cursor.theme:xcursor-breeze
URxvt.size:0
URxvt.cursorBlink:True
URxvt.saveLines:1500
!!边框
URxvt.borderLess:False


!!字体设置
Xft.dpi:96
!消除锯齿
Xft.antialias: true 
!Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
URxvt.font:xft:Source Code Pro:antialias=True:pixelsize=18,xft:WenQuanYi Zen Hei:pixelsize=18
URxvt.boldfont:xft:Source Code Pro:antialias=True:pixelsize=18,xft:WenQuanYi Zen Hei:pixelsize=18

!URxvt.font:xft:Monaco:style=Regular:antialias=True:size=12,xft:Source Han Sans CN:style=Regular:size=12
!URxvt.boldfont:xft:Source Code Pro:style=Regular:antialias=True:size=12,xft:Source Han Sans CN:style=Regular:size=12

!URxvt.font:xft:Source Code Pro:antialias=True:pixelsize=18,xft:WenQuanYi Zen Hei:pixelsize=18
!URxvt.boldfont:xft:Source Code Pro:antialias=True:pixelsize=18,xft:WenQuanYi Zen Hei:pixelsize=18
!URxvt.font:xft:文泉驿等宽微米黑:size=12:style=Regular:antialias=true,xft:文泉驿等宽微米黑:size=11:style=Regular:antialias=true
!URxvt.boldfont:xft:文泉驿等宽微米黑:size=12:style=BOld:antialias=true,xft:文泉驿等宽微米黑:size=11:style=Bold:antialias=true

!!字体动态调节大小
!默认 Ctrl + +/-/=  
URxvt.perl-ext-common:default,resize-font

!! iso 14755/14755_22 mode 
URxvt.iso14755:false
URxvt.iso14755_52:false

!! ctrl + shift + C/V
URxvt.keysym.Shift-Control-C:eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V:eval:paste_clipboard

